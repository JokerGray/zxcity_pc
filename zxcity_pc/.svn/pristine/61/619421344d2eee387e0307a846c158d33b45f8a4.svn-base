li {
  list-style: none;
}
body {
  background: #f4f4f4;
}
body #wrapper {
  width: 1140px;
  margin: 0 auto;
  background: #f2f2f2;
  position: relative;
}
body #wrapper .breadNav {
  width: 786px;
  height: 66px;
  line-height: 66px;
}
body #wrapper .breadNav ol {
  float: left;
  margin: 0;
  padding: 0;
}
body #wrapper .breadNav ol li {
  float: left;
  margin-right: 10px;
}
body #wrapper .breadNav ol li:nth-child(2) {
  width: 10px;
  height: 66px;
  background: url(../img/rightIcon.png) no-repeat center;
}
body #wrapper .breadNav ol li:first-child:before {
  content: '';
  display: inline-block;
  float: left;
  width: 4px;
  height: 24px;
  background-color: #e0041a;
  margin: 21px 5px 0 0;
}
body #wrapper .breadNav ol li a {
  float: left;
  font-size: 20px;
  font-weight: bold;
  color: #666666;
}
body #wrapper .breadNav .mobilePhone {
  float: right;
  width: 134px;
  height: 66px;
  font-size: 12px;
  color: #999999;
  text-align: right;
  background: url(../img/phone.png) no-repeat;
  background-position: left center;
}
body #wrapper .mainLeft {
  width: 786px;
  height: 1000px;
  background: pink;
}
body #wrapper .mainLeft .mainLeft-title {
  width: 786px;
  height: 75px;
  line-height: 75px;
  background-color: #ffffff;
  font-size: 24px;
  color: #333333;
  padding-left: 27px;
}
body #wrapper .mainLeft .mainLeft-bg {
  position: relative;
  width: 786px;
  height: 420px;
  background-image: url(../img/mainImg.png);
  background-repeat: no-repeat;
  background-size: cover;
}
body #wrapper .mainLeft .mainLeft-bg .enroll-num {
  position: absolute;
  left: 486px;
  top: 341px;
  font-size: 22px;
  line-height: 1;
  color: #ffffff;
}
body #wrapper .mainLeft .mainLeft-bg .enroll-num div {
  position: relative;
  width: 275px;
  height: 32px;
  line-height: 32px;
  background-color: #ff4e62;
  border-radius: 10px;
  text-align: center;
}
body #wrapper .mainLeft .mainLeft-bg .enroll-num div:before {
  content: '';
  position: absolute;
  top: 0;
  left: 36px;
  width: 0;
  height: 0;
  border-left: 6px solid transparent;
  border-right: 6px solid transparent;
  border-top: 8px dotted #ffffff;
}
body #wrapper .mainLeft .mainLeft-bg .enroll-num div:after {
  content: '';
}
body #wrapper .sidebar {
  position: absolute;
  top: 66px;
  right: 0;
  width: 328px;
}
body #wrapper .sidebar .issuer {
  position: relative;
  width: 327px;
  height: 151px;
  background-color: #ffffff;
  margin-bottom: 16px;
}
body #wrapper .sidebar .issuer .issuer-Info {
  position: absolute;
  left: 30px;
  top: 30px;
  width: 56px;
  height: 56px;
  background-image: url(../img/headImg.png);
  background-repeat: no-repeat;
  background-size: cover;
}
body #wrapper .sidebar .issuer span:nth-child(2) {
  position: absolute;
  top: 30px;
  left: 100px;
  font-size: 18px;
  color: #cccccc;
}
body #wrapper .sidebar .issuer span:nth-child(3) {
  position: absolute;
  left: 100px;
  top: 55px;
  font-size: 20px;
  color: #333333;
}
body #wrapper .sidebar .issuer span:nth-child(4) {
  position: absolute;
  top: 40px;
  right: 40px;
  font-size: 14px;
  font-weight: bold;
  padding: 5px;
  color: #ff3145;
  border: 1px solid #ff3145;
  border-radius: 5px;
}
body #wrapper .sidebar .hotCircle {
  margin: 5px 0 20px 0;
}
body #wrapper .sidebar .hotCircle .sidebar-title {
  width: 327px;
  height: 50px;
  background-color: #7A70EE;
  background-image: url(../img/seniorityIcon.png);
  background-repeat: no-repeat;
  background-position: 14px center;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
}
body #wrapper .sidebar .hotCircle .sidebar-title .txt {
  font-size: 20px;
  color: #ffffff;
  font-weight: bold;
  margin: 0;
  margin-left: 40px;
  line-height: 47px;
}
body #wrapper .sidebar .hotCircle .sidebar-list li {
  position: relative;
  margin-top: 20px;
}
body #wrapper .sidebar .hotCircle .sidebar-list li .headImg {
  float: left;
  margin: 0 17px 0 14px;
  width: 101px;
  height: 100px;
}
body #wrapper .sidebar .hotCircle .sidebar-list li .info .title {
  font-size: 20px;
  font-weight: bold;
  line-height: 1;
  overflow: hidden;
  -ms-text-overflow: ellipsis;
  text-overflow: ellipsis;
  white-space: nowrap;
}
body #wrapper .sidebar .hotCircle .sidebar-list li .info .userName {
  font-size: 18px;
  color: #cccccc;
  margin-bottom: 28px;
}
body #wrapper .sidebar .hotCircle .sidebar-list li .info .actionStart {
  font-size: 14px;
  font-weight: bold;
  color: #cccccc;
}
body #wrapper .sidebar .hotCircle .sidebar-list li .info .num {
  position: absolute;
  top: -25px;
  left: 15px;
  font-size: 40px;
  color: #ff9ccd;
}
