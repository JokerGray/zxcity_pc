/*******************************************************************/
.appointment-body{
    min-width: 1140px;
    background: #f4f4f4;
    margin-top: 22px;
    margin-left: auto;
    margin-right: auto;
    padding: 0;
}
.appointment-body  .appointment-content{
    width:785px;
    float: left;
}
/*厨师/游泳/健身相关*/
.appointment-content .content-info{
    width:100%;
    background:#fff;
}
.content-info .info-menu{
    width:100%;
    height:114px;
}
.info-menu .btn-group{
    margin-bottom:34px;
    padding-top: 10px;
    font-family: "PingFangSC-Semibold", sans-serif;
}
.btn-group .btn{
    background:#fff;
    color:#333;
    font-size:40px;
    text-align: center;
    margin-left:118.25px !important;
}
.btn-group .btn:hover{
    border-bottom:6px solid #70dec5;
}
.btn-group .active{
    border-bottom:6px solid #70dec5;
}
.info-cook{
    margin: 0 18px;
}
.info-cook .cook-slider{
    width: 100%;
    position: relative;
}
.cook-slider .carousel{
    width:100%;
    height:386px;
}
.cook-slider .carousel .carousel-inner{
    width:100%;
    height:386px;
    overflow: hidden;
    background: #333;
}
.cook-slider .carousel .carousel-control{
    padding: 140px 0;
    font-size: 85px;
    width: 8%;
}
.carousel-inner .item{
    padding: 0 56px;
    height:386px;
    overflow: hidden;
}
.cook-slider .carousel-inner .item img{
    display: block;
    height:386px;
    margin: 0 auto;
}
.cook-slider .cookTitle{
    position: absolute;
    bottom: 0;
    width: 100%;
    font-size: 34px;
    text-align: center;
    height:66px;
    line-height: 66px;
    color:#fff;
    background: rgba(0,0,0,0.5);
    font-family: "PingFangSC-Medium", sans-serif;
}
.cook-slider .cookPage{
    position: absolute;
    right:16px;
    top:28px;
    background: rgba(0,0,0,0.5);
    border-radius: 10px;
    text-align: center;
    padding: 14px 22px;
    color:#fff;
    font-size: 34px;
    font-family: "PingFangSC-Heavy", sans-serif;
}
.info-cook .infoDetail{
    padding:30px 62px 0 15px;
    font-size: 26px;
}
.infoDetail .infoDetail-ul{
    width:100%;
    height:108px;
}

.infoDetail .infoDetail-ul li{
    width:50%;
    float:left;
    margin-bottom: 28px;
    color:#333;
    font-family: "PingFangSC-Medium", sans-serif;
}
.infoDetail-ul li .star{
    display: inline-block;
}
.infoDetail-ul li .star i{
    display: block;
    float: left;
    width: 28px;
    height: 28px;
    margin-right: 5px;
}
.infoDetail-ul li .star .yellowStar{
    background: url("../img/yellowStar.png") no-repeat;
    background-size: 100%;
}
.infoDetail-ul li .star .grayStar{
    background: url("../img/grayStar.png") no-repeat;
    background-size: 100%;
}
.infoDetail-ul li span{
    color:#999;
}
.infoDetail dl{
    width:672px;
    border-bottom:1px solid #dadada;
    font-family: "PingFangSC-Medium", sans-serif;
}
.infoDetail dl dt{
    margin-bottom: 28px;
    color:#333;
    float: left;
    width: 130px;
}
.infoDetail dl dd{
    margin-bottom: 28px;
    color:#999;
    width:542px;
    margin-left: 130px;
}
.infoDetail dl dd button{
    background: #fff;
    font-size: 26px;
    padding: 0;
}
.infoDetail dl dd button:first-child{
    margin-right:10px;
}
.content-info .share{
    padding:0 33px;
    height: 64px;
    line-height: 64px;
}
.content-info .share span{
    color:#666;
    font-size: 18px;
    float:left;
    font-family: 'PingFangSC-Regular', sans-serif;
}
.share .shareList li{
    width:27px;
    float:left;
    margin-right:10px;
    margin-top: 18.5px;
}
.shareList li a{
    display:block;
    width:27px;
    height: 26px;
}
.shareList li .qzone{
    background: url("../img/qzone.png") no-repeat;
}
.shareList li .sina{
    background: url("../img/sina.png") no-repeat;
}
.shareList li .qq{
    background: url("../img/qq.png") no-repeat;
}
/*评论*/
.appointment-content .content-conment{
    width:100%;
    margin:16px 0 110px;
    background: #fff;
    padding-bottom: 24px;
}
.content-conment .conmentTit{
    width:279px;
    padding:28px 0;
    background: url("../img/fenge.png") no-repeat;
    background-position: center;
    margin: 0 auto;
}
.conmentTit h3{
    width:279px;
    text-align: center;
    font-size: 40px;
    font-family: "PingFangSC-Heavy", sans-serif;
}
.content-conment .conmentDetail{
    padding:0 33px;
}
.conmentDetail .conmentDetail-info{
    border-top:1px solid #dadada;
    padding:38px 0 28px;
    width:672px;
}
.conmentDetail-info .conmentDetail-head{
    width:84px;
    float: left;
    margin-right: 20px;
}
.conmentDetail-info .conmentDetail-head img{
    display:block;
    width:84px;
    height:84px;
    overflow: hidden;
    border:6px solid #70dec5;
    border-radius: 100%;
}
.conmentDetail-info .conmentDetail-article{
    margin-left:104px;
}
.conmentDetail-article .article-nameInof{
    padding-top:6px;
    height: 34px;
    margin-bottom: 20px;
}
.article-nameInof .nameInfo-username{
    font-size: 24px;
    color:#333;
    display: inline-block;
    float: left;
    font-family: "PingFangSC-Medium", sans-serif;
}
.article-nameInof .nameInfo-username i{
    display: block;
    width:27px;
    height: 28px;
    float: right;
}
.article-nameInof .nameInfo-username span {
    float: left;
}
.nameInfo-username .man{
    background: url("../img/man.png") no-repeat;
    background-position: center;
    background-size: 60%;
}
.article-nameInof .nameInfo-star{
    display: block;
    float: right;
}
.nameInfo-star i{
    display: block;
    width:26px;
    height: 26px;
    float: left;
    margin-left: 5px;
}
.nameInfo-star .yellowStar {
    background: url("../img/yellowStar.png") no-repeat;
    background-size: 100%;
}
.nameInfo-star .grayStar {
    background: url("../img/grayStar.png") no-repeat;
    background-size: 100%;
}
.conmentDetail-article .conment-date{
    font-size: 24px;
    color: #666;
    font-family: 'PingFangSC-Regular', sans-serif;
    margin-bottom:28px;
}
.conmentDetail-article .conment-text{
    font-family: 'PingFangSC-Regular', sans-serif;
    color:#999;
    font-size: 24px;
    line-height: 34px;
}
.conmentDetail .lookMore{
    width: 100%;
    display: block;
    font-family: 'PingFangSC-Regular', sans-serif;
    font-size: 14px;
    color:#333;
    height: 46px;
    line-height: 46px;
    background:#eeeeee;
    padding: 0;
}
/*侧边栏*/
.appointment-sidebar{
    float: right;
    width:328px;
}
.appointment-sidebar .sidebar-userInfo{
    width:100%;
    height: 193px;
    overflow: hidden;
    position: relative;
    margin-bottom: 21px;
}
.sidebar-userInfo .userInfo-background{
    position: absolute;
    width: 100%;
    height: 193px;
    overflow: hidden;
    z-index: 1;
}
.userInfo-background img{
    display: block;
    width: 100%;
    height: 193px;
}
.userInfo-background .userInfo-mask{
    position: absolute;
    top: 0;
    width: 100%;
    height: 193px;
    background: #000;
    filter:alpha(opacity=50);
    -moz-opacity:0.5;
    -khtml-opacity: 0.5;
    opacity: 0.5;
}
.sidebar-userInfo img{
    display: block;
    width:90px;
    height: 90px;
    overflow: hidden;
    border:6px solid #70dec5;
    border-radius: 100%;
    margin:10px auto 20px;
}
.userInfo-infomation{
    position: absolute;
    width: 100%;
    z-index: 2;
    text-align: center;
}
.userInfo-infomation .nameInfo-username{
    display: inline-block;
    color: #fff;
    font-family: "PingFangSC-Medium", sans-serif;
    font-size: 19.4px;
    padding: 0 5px;
    margin-bottom: 20px;
}
.userInfo-infomation .nameInfo-username i{
    display: block;
    width: 25px;
    height: 23px;
    float: right;
}
.userInfo-infomation .phoneTest,.userInfo-infomation .idTest{
    clear: both;
    color:#fff;
    font-size: 17.9px;
    font-family: "PingFangSC-Semibold", sans-serif;
    display: inline-block;
}
.userInfo-infomation .phoneTest i{
    display: block;
    float: left;
    width:16px;
    height: 22px;
    background: url("../img/phone.png") no-repeat;
    background-size: 80%;
}
.userInfo-infomation .idTest i{
    display: block;
    float: left;
    width:30px;
    height: 22px;
    background: url("../img/id.png") no-repeat;
    background-size: 80%;
}
.userInfo-infomation .phoneTest{
    margin-right: 45px;
}
.appointment-sidebar .sidebar-hotSale{
    height: auto;
    width: 100%;
}
.sidebar-hotSale h4{
    height: 50px;
    line-height: 50px;
    color: #fff;
    background: #7a70ee;
    border-radius: 5px 5px 0 0;
    padding:0 14px;
    font-size: 20px;
    font-family: "Microsoft YaHei";
}
.sidebar-hotSale h4 .topIocn{
    display: block;
    width: 13px;
    height: 20px;
    background: url("../img/topIcon.png") no-repeat;
    float: left;
    margin-top: 15px;
    margin-right: 12px;
}
.sidebar-hotSale .hotSaleList{
    width: 100%;
}
.sidebar-hotSale .hotSaleList li{
    padding: 19px 14px 6px;
    background: #fff;
    height: 125px;
    position: relative;
    overflow: hidden;
}
.hotSaleList li .topHead{
    width: 100px;
    height: 100px;
    float: left;
}
.hotSaleList li .topHead i{
    display: block;
    width: 30px;
    height: 40px;
    position: absolute;
    top: 2px;
}
.topHead .topNum1{
    background: url("../img/topNum1.png") no-repeat;
}
.topHead .topNum2{
    background: url("../img/topNum2.png") no-repeat;
}
.topHead .topNum3{
    background: url("../img/topNum3.png") no-repeat;
}
.topHead .topNum4{
    background: url("../img/topNum4.png") no-repeat;
}
.topHead .topNum5{
    background: url("../img/topNum5.png") no-repeat;
}
.hotSaleList li .topHead img{
    display: block;
    width: 100px;
    height: 100px;
    border: 1px solid #efefef;
}
.hotSaleList li .topInfo{
    margin-left: 117px;
}
.topInfo .topInfo-message{
    font-size: 20px;
    color:#666;
    font-family: "Microsoft YaHei";
    width: 183px;
    overflow:hidden;
    text-overflow:ellipsis;
    -o-text-overflow:ellipsis;
    -webkit-text-overflow:ellipsis;
    -moz-text-overflow:ellipsis;
    white-space:nowrap;
}
.topInfo .topInfo-userName{
    font-size: 18px;
    color:#ccc;
    font-family: "Microsoft YaHei";
    margin:11px 0 31px;
}
.topInfo .fans{
    display: inline-block;
    float: left;
    margin-right: 24px;
    font-family: "Microsoft YaHei";
    font-size: 18px;
    color:#666;
}
.topInfo .fans i{
    display: block;
    width: 18px;
    height: 18px;
    background: url("../img/heart.png") no-repeat;
    float: left;
    margin-right: 5px;
}
.topInfo .service{
    display: inline-block;
    font-family: "Microsoft YaHei";
    font-size: 18px;
    color:#666;
}
.topInfo .service i{
    display: block;
    width: 20px;
    height: 18px;
    margin-right: 5px;
    background: url("../img/talk.png") no-repeat;
    float: left;
}
.appointment-sidebar .sidebar-fierce{
    height: auto;
    width: 100%;
    margin-top:30px;
}
.sidebar-fierce h4{
    height: 50px;
    line-height: 50px;
    color: #fff;
    background: #ff808c;
    border-radius: 5px 5px 0 0;
    padding:0 14px;
    font-size: 20px;
    font-family: "Microsoft YaHei";
}
.sidebar-fierce h4 .topIocn{
    display: block;
    width: 13px;
    height: 20px;
    background: url("../img/topIcon.png") no-repeat;
    float: left;
    margin-top: 15px;
    margin-right: 12px;
}
.sidebar-fierce .fierceList{
    width: 100%;
}
.sidebar-fierce li{
    padding: 19px 14px 6px;
    background: #fff;
    height: 125px;
    position: relative;
    overflow: hidden;
}
.sidebar-fierce li .topHead{
    width: 100px;
    height: 100px;
    float: left;
}
.sidebar-fierce li .topHead i{
    display: block;
    width: 30px;
    height: 40px;
    position: absolute;
    top: 2px;
}
.sidebar-fierce li .topHead img{
    display: block;
    width: 100px;
    height: 100px;
}
.sidebar-fierce li .topInfo{
    margin-left: 117px;
}