/**
 * Eric Meyer's Reset CSS v2.0 (http://meyerweb.com/eric/tools/css/reset/)
 * http://cssreset.com
 */
html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, menu, nav, output, ruby, section, summary, time, mark, audio, video, input {
    margin: 0;
    padding: 0;
    border: 0;
    font-weight: normal;
    font-style: normal;
}

/* HTML5 display-role reset for older browsers */
article, aside, details, figcaption, figure,
footer, header, menu, nav, section {
    display: block;
}

body {
    font-family: '微软雅黑', sans-serif;
    line-height: 1;
    background: #f4f4f4;
}

table {
    border-collapse: collapse;
    border-spacing: 0;
}

a {
    color: #7e8c8d;
    text-decoration: none;
    -webkit-backface-visibility: hidden;
}

li {
    list-style: none;
}

/*******************************************************************/
.header {

    background-position: 50% 100%;
    background-repeat: no-repeat;
    background-color: #ffffff;
}

#main {
    width: 1140px;
    margin: 0 auto 120px auto;
    background-color: #ffffff;
    .header {
        width: 1140px;
        height: 120px;
        margin: 0 auto 80px auto;
        background-image: url(../img/title.png);
    }
    .container {
        width: 960px;
        margin: 0 auto;
        padding-bottom: 80px;
        div {
            .a {
                font-weight: bold;
                font-size: 45px;
            }
            .b {
                font-weight: bold;
                font-size: 36px;
            }
            .c {
                font-weight: bold;
                font-size: 28px;
            }
            .d {
                font-weight: bold;
                font-size: 21px;
            }
        }
        .icon {
            width: 201px;
            height: 23px;
            background-image: url(../img/icon.png);
            background-repeat: no-repeat;
            margin: 10px 0 10px -30px;
        }
        .jishu {
            text-align: left;
            font-size: 21px;
            margin-bottom: 45px;
            td:nth-child(1) {
                line-height: 1.5;
                width: 250px;
                font-weight: bold;
            }
            td:nth-child(2) {
                line-height: 1.5;
                width: 320px;
                font-weight: bold;
            }
            td:nth-child(3) {
                line-height: 1.5;
                font-weight: bold;
            }
        }
        .yunying {
            text-align: left;
            font-size: 21px;
            margin-bottom: 45px;
            td:nth-child(1) {
                width: 480px;
                line-height: 1.5;
                font-weight: bold;
            }
            td:nth-child(2) {
                width: 480px;
                line-height: 1.5;
                font-weight: bold;
            }
        }
        .pm {
            text-align: left;
            font-size: 21px;
            margin-bottom: 45px;
            td:nth-child(1) {
                width: 640px;
                line-height: 1.5;
                font-weight: bold;
            }
            td:nth-child(2) {
                width: 320px;
                line-height: 1.5;
                font-weight: bold;
            }
        }
        .info {
            width: 880px;
            font-size: 21px;
            line-height: 1.5;
            color: #afa6a7;
        }
    }
    .footer {
        width: 1140px;
        margin: 0 auto;
        line-height: 1.5;
        padding: 0 0 75px 0;
        p {
            width: 960px;
            margin: 0 auto;
            font-size: 21px;
            font-weight: bold;
            i {
                font-size: 27px;
                font-weight: bold;
            }
        }
    }
}


