li {
  list-style: none;
}
body {
  background: #f4f4f4;
}
#wrapper {
  width: 1140px;
  margin: 0 auto;
  background: #f2f2f2;
  position: relative;
}
#wrapper .mainLeft {
  width: 786px;
  background: #ffffff;
}
#wrapper .mainLeft .tabs {
  width: 786px;
  height: 103px;
  line-height: 103px;
  text-align: center;
}
#wrapper .mainLeft .tabs a {
  margin-right: 42px;
  font-size: 30px;
  color: #999999;
}
#wrapper .mainLeft .tabs a.active {
  font-size: 40px;
  color: #333333;
  border-bottom: 5px solid #333333;
}
#wrapper .mainLeft .content-box .content-1 {
  display: none;
}
#wrapper .mainLeft .content-box .content-1 .title {
  width: 786px;
  height: 100px;
  padding: 0 41px;
}
#wrapper .mainLeft .content-box .content-1 .title h2 {
  float: left;
  margin: 0;
  line-height: 100px;
  text-align: left;
  font-size: 44px;
  font-weight: bold;
}
#wrapper .mainLeft .content-box .content-1 .title .share {
  font-size: 18px;
  float: right;
  line-height: 100px;
  margin-right: -25px;
}
#wrapper .mainLeft .content-box .content-1 .title .share span {
  color: #666666;
  margin-right: 10px;
}
#wrapper .mainLeft .content-box .content-1 .title .share a {
  display: inline-block;
  width: 26px;
  height: 26px;
  margin-right: 10px;
}
#wrapper .mainLeft .content-box .content-1 .title .share a:nth-child(2) {
  vertical-align: middle;
  background: url(../img/qqSpace.png) no-repeat;
}
#wrapper .mainLeft .content-box .content-1 .title .share a:nth-child(3) {
  vertical-align: middle;
  background: url(../img/weibo.png) no-repeat;
}
#wrapper .mainLeft .content-box .content-1 .title .share a:nth-child(4) {
  vertical-align: middle;
  background: url(../img/qq.png) no-repeat;
}
#wrapper .mainLeft .content-box .content-1 .shop {
  padding: 0 31px 0 41px;
}
#wrapper .mainLeft .content-box .content-1 .shop .shopInfo {
  float: left;
  margin-right: 10px;
}
#wrapper .mainLeft .content-box .content-1 .shop .shopInfo img {
  display: block;
  width: 346px;
  height: 346px;
}
#wrapper .mainLeft .content-box .content-1 .shop .shopInfo .goodsName {
  width: 346px;
  font-size: 28px;
  font-weight: bold;
  color: #3e3e3e;
  overflow: hidden;
  -ms-text-overflow: ellipsis;
  text-overflow: ellipsis;
  white-space: nowrap;
}
#wrapper .mainLeft .content-box .content-1 .shop .shopInfo .shopName {
  width: 346px;
  font-size: 24px;
  color: #999999;
}
#wrapper .mainLeft .content-box .content-1 .shop .shopInfo .price {
  font-size: 16px;
  color: #ff3b3c;
}
#wrapper .mainLeft .content-box .content-1 .shop .shopInfo .price .num {
  font-size: 26px;
}
#wrapper .mainLeft .content-box .content-1 .shop:after {
  display: table;
  content: '';
  clear: both;
}
#wrapper .mainLeft .content-box .content-2 {
  display: block;
}
#wrapper .mainLeft .content-box .content-2 .title {
  width: 786px;
  height: 100px;
  padding: 0 41px;
}
#wrapper .mainLeft .content-box .content-2 .title h2 {
  float: left;
  margin: 0;
  line-height: 100px;
  text-align: left;
  font-size: 44px;
  font-weight: bold;
}
#wrapper .mainLeft .content-box .content-2 .title .share {
  font-size: 18px;
  float: right;
  line-height: 100px;
  margin-right: -25px;
}
#wrapper .mainLeft .content-box .content-2 .title .share span {
  color: #666666;
  margin-right: 10px;
}
#wrapper .mainLeft .content-box .content-2 .title .share a {
  display: inline-block;
  width: 26px;
  height: 26px;
  margin-right: 10px;
}
#wrapper .mainLeft .content-box .content-2 .title .share a:nth-child(2) {
  vertical-align: middle;
  background: url(../img/qqSpace.png) no-repeat;
}
#wrapper .mainLeft .content-box .content-2 .title .share a:nth-child(3) {
  vertical-align: middle;
  background: url(../img/weibo.png) no-repeat;
}
#wrapper .mainLeft .content-box .content-2 .title .share a:nth-child(4) {
  vertical-align: middle;
  background: url(../img/qq.png) no-repeat;
}
#wrapper .mainLeft .content-box .content-2 .action {
  padding: 0 18px;
  margin-bottom: 33px;
}
#wrapper .mainLeft .content-box .content-2 .action .actionInfo {
  position: relative;
  width: 750px;
  height: 420px;
  margin-bottom: 36px;
  background-image: url(../img/actionInfo.png);
  background-repeat: no-repeat;
  -webkit-background-size: cover;
  background-size: cover;
}
#wrapper .mainLeft .content-box .content-2 .action .actionInfo .titleName {
  padding: 0 0 0 10px;
  height: 60px;
  line-height: 60px;
  background-color: rgba(0, 0, 0, 0.5);
  position: absolute;
  right: 0;
  top: 20px;
}
#wrapper .mainLeft .content-box .content-2 .action .actionInfo .titleName i {
  display: inline-block;
  width: 28px;
  height: 22px;
  background-image: url(../img/titleIcon.png);
  background-repeat: no-repeat;
  vertical-align: middle;
}
#wrapper .mainLeft .content-box .content-2 .action .actionInfo .titleName span {
  font-size: 24px;
  color: #ffffff;
  vertical-align: middle;
  margin: 0 24px 0 5px;
}
#wrapper .mainLeft .content-box .content-2 .action .actionInfo .titleName:before {
  content: '';
  width: 4px;
  height: 60px;
  position: absolute;
  top: 0;
  bottom: 0;
  left: -10px;
  background-color: #636F7F;
}
#wrapper .mainLeft .content-box .content-2 .action .actionInfo .price {
  width: 180px;
  height: 90px;
  position: absolute;
  left: 24px;
  bottom: 20px;
}
#wrapper .mainLeft .content-box .content-2 .action .actionInfo .price .cur-price {
  height: 55px;
  line-height: 55px;
  background-color: #ff665f;
  border-radius: 10px 10px 0 0;
  text-align: center;
  font-size: 36px;
  color: #ffffff;
}
#wrapper .mainLeft .content-box .content-2 .action .actionInfo .price .cos-price {
  height: 35px;
  line-height: 35px;
  background-color: #484848;
  border-radius: 0 0 10px 10px;
  font-size: 22px;
  color: #ffffff;
  text-align: center;
  text-decoration: line-through;
}
#wrapper .mainLeft .content-box .content-2 .action .actionInfo .countDown {
  position: absolute;
  left: 224px;
  bottom: 20px;
  height: 80px;
  font-size: 32px;
  text-align: center;
  color: #ffffff;
  line-height: 1;
}
#wrapper .mainLeft .content-box .content-2 .action .actionInfo .countDown p {
  text-align: left;
  font-size: 22px;
  color: #ffffff;
  letter-spacing: 2px;
  line-height: 1.1;
}
#wrapper .mainLeft .content-box .content-2 .action .actionInfo .countDown span {
  display: inline-block;
  width: 55px;
  height: 45px;
  border: 1px solid #ffffff;
  border-radius: 5px;
  line-height: 45px;
  vertical-align: middle;
}
#wrapper .mainLeft .content-box .content-2 .action .actionInfo .enroll {
  position: absolute;
  right: 25px;
  bottom: 20px;
  font-size: 24px;
  color: #ffffff;
}
#wrapper .mainLeft .content-box .content-2 .action .action-txt {
  font-size: 40px;
  color: #333333;
  text-indent: 24px;
}
#wrapper .mainLeft .content-box .content-2 .action .action-date {
  font-size: 30px;
  color: #999999;
  text-indent: 24px;
  margin-bottom: 20px;
}
#wrapper .mainLeft .content-box .content-2 .action .support {
  font-size: 24px;
  color: #ff4e62;
  text-indent: 24px;
}
#wrapper .mainLeft .content-box .content-2 .action .support .boss {
  padding: 0 2px;
  border: 1px solid #ff4e62;
  margin-right: 20px;
}
#wrapper .mainLeft .content-box .content-2 .action .support .seller {
  padding: 0 2px;
  border: 1px solid #ff4e62;
}
#wrapper .sidebar {
  position: absolute;
  top: 0;
  right: 0;
  width: 328px;
}
#wrapper .sidebar .sidebar-adv {
  background-color: #fff;
}
#wrapper .sidebar .sidebar-adv .adv {
  position: relative;
  width: 329px;
  height: 212px;
  background-size: contain;
  background-repeat: no-repeat;
}
#wrapper .sidebar .sidebar-adv .adv:nth-child(1) {
  margin-top: 23px;
  background-image: url(../img/adv-1.png);
}
#wrapper .sidebar .sidebar-adv .adv:nth-child(2) {
  margin-top: 23px;
  background-image: url(../img/adv-2.png);
}
#wrapper .sidebar .sidebar-adv .adv:nth-child(3) {
  background-image: url(../img/adv-3.png);
}
#wrapper .sidebar .storeChoice {
  margin-top: 5px;
}
#wrapper .sidebar .storeChoice .sidebar-title {
  width: 327px;
  height: 50px;
  background-color: #7A70EE;
  background-image: url(../img/title.png);
  background-repeat: no-repeat;
  background-position: 14px center;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
}
#wrapper .sidebar .storeChoice .sidebar-title .txt {
  font-size: 20px;
  color: #ffffff;
  font-weight: bold;
  margin: 0;
  margin-left: 40px;
  line-height: 47px;
}
#wrapper .sidebar .storeChoice .sidebar-list li {
  margin-top: 20px;
}
#wrapper .sidebar .storeChoice .sidebar-list li .headImg {
  float: left;
  margin: 0 17px 0 14px;
  width: 101px;
  height: 100px;
}
#wrapper .sidebar .storeChoice .sidebar-list li .info .title {
  font-size: 20px;
  font-weight: bold;
  line-height: 1;
  overflow: hidden;
  -ms-text-overflow: ellipsis;
  text-overflow: ellipsis;
  white-space: nowrap;
}
#wrapper .sidebar .storeChoice .sidebar-list li .info .userName {
  font-size: 18px;
  color: #cccccc;
  margin-bottom: 23px;
}
#wrapper .sidebar .storeChoice .sidebar-list li .info .praise {
  background-image: url(../img/dianzan.png);
  background-repeat: no-repeat;
  background-position: 43%;
}
#wrapper .sidebar .storeChoice .sidebar-list li .info .praise .count {
  margin-left: 25px;
  color: #666666;
  font-size: 18px;
  font-weight: bold;
}
