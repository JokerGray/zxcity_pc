ul,li{
    margin:0;
    padding:0;
    list-style: none;
}
img{
    width:100%;
    height:100%;
}
i{
    font-style: normal;
}
a, a:hover, a:active, a:focus, a:link{
    text-decoration: none;
}
html{
    height:100%;
    overflow-y:auto;
}
body{
    position:relative;
    font-family: 'microsoft yahei',Helvetica,Arial,sans-serif;
    min-height: 100%;
    padding-bottom:235px;
}
/**
 * BootstrapValidator (http://bootstrapvalidator.com)
 * The best jQuery plugin to validate form fields. Designed to use with Bootstrap 3
 */
.bv-form .help-block {
    margin-bottom: 0;
}
.bv-form .tooltip-inner {
    text-align: left;
}
.nav-tabs li.bv-tab-success > a {
    color: #3c763d;
}
.nav-tabs li.bv-tab-error > a {
    color: #a94442;
}
.bv-form .bv-icon-no-label {
	top: 0;
}
.bv-form .bv-icon-input-group {
	top: 0;
	z-index: 100;
}
/**header css*/
header, .header{
    height:auto;
    width:100%;
    overflow:hidden;
}

.header-top{
    height:52px;
    line-height: 52px;
    background-color: #433f50;
}
.header-top .logo{
    width:127px;
    height:54px;
    margin:0;
    background:url(../images/logo.png) no-repeat center;
}
.header-top .link-btn{
    width:120px;
}
.header-top a,
.header-top span{
    font-size:18px;
    color:#eee;
}
.login-info .user-pic{
    display:inline-block;
    width:30px;
    height:30px;
    border-radius:100%;
}
.login-info .username{
    display:inline-block;
    margin-left:5px;
    margin-right:10px;
}
.header-nav{
    height:52px;
    padding-top:5px;
    background-color:#3c3849;
}
.header-nav .nav>li>a{
    padding:8px 15px;
    font-family:microsoft yahei;
    font-size:18px;
    color:#fff;
}
.header-nav .nav>li>a:focus,
.header-nav .nav>li>a:hover{
    background-color:#433f50;
}
.header-sub-nav{
    height:66px;
    background-color: #f4f4f4;
}
.header-sub-nav .main-left{
    display:flex;
    height:66px;
    align-items: center;
    justify-content: space-between;
}
.header-sub-nav .sub-nav-title{
    color:#666;
    border-left:5px solid #e0041a;
    padding-left:10px;
}
.header-sub-nav .sub-nav-title b{
    font-size:20px;
}
.header-sub-nav .sub-nav-title i{
    margin-left:5px;
    margin-right:5px;
}

.header-sub-nav .more-link{
    display:inline-block;
    width:134px;
    height:30px;
    background: url(../images/more_link.png) no-repeat;
}
/**footer css*/
footer, .footer{
    position:absolute;
    bottom:0;
    width:100%;
    height:235px;
    color:#fff;
}
.footer .footer-link{
    height:195px;
    padding-top:30px;
    background-color: #433f50;
    text-align: center;
}
.footer .footer-link p{
    width:882px;
    height:132px;
    background: url(../images/footer-link-icon.png) no-repeat center;
}
.footer .copyright{
    height:40px;
    line-height: 40px;
    background-color:#3c3849;
    text-align: center;
}

/**pop-box*/
.pop-box{
    display:none;
    position: fixed;
    top:0;
    left:0;
    right:0;
    bottom:0;
    width:100%;
    height:100%;
    z-index:99;
}
.pop-mask{
    position: absolute;
    top:0;
    left:0;
    right:0;
    bottom:0;
    background-color: rgba(0,0,0,0.5);
    background-color: #000;
    opacity: 0.5;
    filter: alpha(opacity=50);
}
.pop-inner{
    position:absolute;
    top:50%;
    left:50%;
    width:500px;
    height:500px;
    margin-top:-250px;
    margin-left:-250px;
    padding-left:70px;
    padding-right:70px;
    background-color: #fff;
    z-index:101;
}
.pop-inner .pop-title{
    margin-top:35px;
    margin-bottom:35px;
    font-size: 22px;
    color:#666;
    text-align: center;
}
.pop-inner .close-btn{
	position:absolute;
	top:5px;
	right:5px;
	width:32px;
	height:32px;
    background:url(../images/close_btn.png) no-repeat 0 0;
}
.pop-inner .pop-con{
    color:#666;
}
.pop-inner .pop-con input[type=text],
.pop-inner .pop-con input[type=password]{
    height:46px;
    border:1px solid #dcdcdc;
    margin-bottom:12px;
}
.pop-inner .pop-con input[type=checkbox]{
    width:22px;
    height:22px;
    margin-top:0px;
    margin-left: -25px;
}
.pop-inner .pop-con .checkbox label{
    padding-left:25px;
}
.pop-inner .pop-con button{
    width:100%;
    height:46px;
    font-size:16px;
}
.pop-inner .pop-con a{
    color: currentColor;
}
.pop-inner .pop-con .submit-btn{
    margin-top:20px;
    background-color: #fe6869;
    color:#fff;
}
.pop-inner .pop-con .change-btn{
    background-color: #eee;
    color:#fe6869;
}
.pop-con .security-code button{
    width:138px;
    background-color: #68affe;
    color:#fff;
    text-align:center;
}
.pop-con .security-code input[type=text]{
    display:inline-block;
    width:235px;
    margin-left:12px;
}
.pop-con .login-btn,
.pop-con .register-btn,
.pop-con .submit-btn{
    background-color:#fe6869;
    color:#fff;
}


@keyframes jump {
    0% {
        transform: translate(0, 0);   }
    25% {
        transform: translate(0, 0.1rem);   }
    50% {
        transform: translate(0, 0);   }
    75% {
        transform: translate(0, -0.1rem);   }
    100% {
        transform: translate(0, 0);   }
}

.main-left{
    float:left;
    width:786px;
    overflow:hidden;
}
.main-right{
    float:right;
    width:328px;
    overflow:hidden;
}
